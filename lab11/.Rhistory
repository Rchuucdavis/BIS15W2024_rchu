ggplot(aes(x=taxon, fill(trophic.guild)))+
geom_bar()
homerange %>%
ggplot(aes(x=taxon, fill=trophic.guild))+
geom_bar()
homerange %>%
ggplot(aes(x=taxon, fill=trophic.guild))+
geom_bar()+
coord_flip
homerange %>%
ggplot(aes(x=taxon, fill=trophic.guild))+
geom_bar()+
coord_flip()
homerange %>%
ggplot(aes(x=taxon, fill=trophic.guild))+
geom_bar(position = "dodge")+
coord_flip()
